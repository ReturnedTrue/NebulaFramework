{
  "name": "NebulaFramework",
  "tree": {
    "$className": "DataModel",

    "ServerScriptService": {
      "Nebula": {
        "$path": "tests/Server"
      },

      "NebulaInternal": {
          "$path": "src/Server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Nebula": {
          "$path": "tests/Client"
        },

        "NebulaInternal": {
            "$path": "src/Client"
        }        
      }
    },
    
    "ReplicatedStorage": {
      "Tools": {
        "$path": "tests/Tools"
      },

      "Nebula": {
        "$path": "tests/Shared"
      },

      "NebulaInternal": {
        "$path": "src/Shared"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },

    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },

    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}